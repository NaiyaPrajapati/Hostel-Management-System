}

<h1>View Menu</h1>

<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BreakFastItem1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BreakFastItem2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BreakFastItem3)
