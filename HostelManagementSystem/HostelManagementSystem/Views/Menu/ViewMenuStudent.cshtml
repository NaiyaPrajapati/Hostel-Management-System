            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DinnerItem3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DinnerItem4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DinnerItem5)
            </td>
