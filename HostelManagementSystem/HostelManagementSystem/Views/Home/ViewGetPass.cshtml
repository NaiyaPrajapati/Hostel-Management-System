@model IEnumerable<HostelManagementSystem.Models.GetPass>

@{
    ViewData["Title"] = "ViewGetPass";
}
<center>
<h4>Requested Gate Passes</h4>
    </center>

<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.StudentId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Place)
            </th>
            <th>
               Departure Date-Time @*@Html.DisplayNameFor(model => model.Dept_date)*@
            </th>
            <th>
               Arrival Date-Time @*@Html.DisplayNameFor(model => model.Arrival_date)*@
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.StudentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dept_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Arrival_date)
            </td>
            <td>
                @Html.ActionLink("Approve", "UpdateGetPass", new {  id=item.id }) |
             
                @Html.ActionLink("Reject", "DeleteGetPass", new { id=item.id })
            </td>
        </tr>
}
    </tbody>
</table>
